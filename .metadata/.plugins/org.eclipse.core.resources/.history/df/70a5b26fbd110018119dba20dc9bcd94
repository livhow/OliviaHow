import static java.lang.System.*;

public class Triples
{
   private int number;

	public Triples()
	{
		this(0);
	}

	public Triples(int num)
	{
//a2 + b2 == c2
//if a == odd, b==even, a ==even, b == odd, c == always odd
//GCF can be <= 1
		number = 0;
	}

	public void setNum(int num)
	{
		number = num;

	}
	
	private int greatestCommonFactor(int a, int b, int c)
	{
		int max = 0;
		return 1;
	}

	public String toString()
	{
		String output="";
		int a = 0, b = 0, c = 0;
		while (c < number)
		{
			int m = 2;
			for(int i = 1; i < m; i++ )
			{
				a = (m*m) - (i*i);
	            b = m*i;
	            b  =2 * b;
	            c = (m*m) + (i*i);
			}
			if (c > number)
			{
				break;
			}
			output = output + (a + " " + b + " " + c + " ");
			m++;
		}
		return output+"\n";
	}
}